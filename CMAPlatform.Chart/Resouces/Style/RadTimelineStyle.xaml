        <Setter Property="BorderThickness"
                Value="{StaticResource Timeline_Button_Bottom_Thickness}" />
        <Setter Property="Background"
                Value="{StaticResource Timeline_Button_Bottom_Background}" />
        <Setter Property="Height"
                Value="0" />
    </Style>
    <Style x:Key="TimelineItemControlStyle"
           TargetType="telerik:TimelineItemControl">
        <Setter Property="Height"
                Value="10" />
        <Setter Property="BorderBrush"
                Value="{StaticResource Timeline_TimelineItem_Stroke}" />
        <Setter Property="Margin"
                Value="0 5 0 0" />
        <Setter Property="Background"
                Value="{StaticResource Timeline_TimelineItem_Fill}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:TimelineItemControl">
                    <Border Background="#01FFFFFF">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem"
                                                                       Storyboard.TargetProperty="Fill"
                                                                       Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                    Value="{StaticResource Timeline_TimelineItem_Fill_MouseOver}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem"
                                                                       Storyboard.TargetProperty="Stroke"
                                                                       Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                    Value="{StaticResource Timeline_TimelineItem_Stroke_MouseOver}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem"
                                                                       Storyboard.TargetProperty="Fill"
                                                                       Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                    Value="{StaticResource Timeline_TimelineItem_Fill_Pressed}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem"
                                                                       Storyboard.TargetProperty="Stroke"
                                                                       Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                    Value="{StaticResource Timeline_TimelineItem_Stroke_Pressed}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="timelineItem"
                                   Margin="0 .5 0 .5"
                                   Fill="{TemplateBinding Background}"
                                   Stroke="{TemplateBinding BorderBrush}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineInstantItemControlStyle"
           TargetType="telerik:TimelineInstantItemControl">
        <Setter Property="Margin"
                Value="0 5 0 0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:TimelineInstantItemControl">
                    <Border Background="#01FFFFFF"
                            Width="10"
                            Height="10">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem"
                                                                       Storyboard.TargetProperty="Fill"
                                                                       Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                    Value="{StaticResource Timeline_TimelineInstantItem_Fill_MouseOver}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem"
                                                                       Storyboard.TargetProperty="Stroke"
                                                                       Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                    Value="{StaticResource Timeline_TimelineInstantItem_Stroke_MouseOver}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem"
                                                                       Storyboard.TargetProperty="Fill"
                                                                       Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                    Value="{StaticResource Timeline_TimelineInstantItem_Fill_Pressed}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem"
                                                                       Storyboard.TargetProperty="Stroke"
                                                                       Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                    Value="{StaticResource Timeline_TimelineInstantItem_Stroke_Pressed}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="timelineItem"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   StrokeThickness="{StaticResource Timeline_TimelineInstantItem_Stroke_Thickness}"
                                   Height="7"
                                   Width="7"
                                   Fill="{StaticResource Timeline_TimelineInstantItem_Fill}">
                            <Rectangle.RenderTransform>
                                <RotateTransform Angle="45"
                                                 CenterX="3.5"
                                                 CenterY="3.5" />
                            </Rectangle.RenderTransform>
                        </Rectangle>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="TimelineItemTemplate">
        <telerik:TimelineItemControl />
    </DataTemplate>
    <DataTemplate x:Key="TimelineInstantItemTemplate">
        <telerik:TimelineInstantItemControl />
    </DataTemplate>
    <Style x:Key="TimelineAnnotationStyle"
           TargetType="telerik:TimelineAnnotation">
        <Setter Property="IsHitTestVisible"
                Value="False" />
        <Setter Property="Margin"
                Value="{StaticResource Timeline_TimelineAnnotation_Margin}" />
        <Setter Property="Canvas.ZIndex"
                Value="300" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:TimelineAnnotation">
                    <ContentPresenter Content="{TemplateBinding Content}"
                                      ContentTemplate="{TemplateBinding ContentTemplate}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SelectionRegionStyle"
           TargetType="telerik:SelectionRegion">
        <Setter Property="Visibility"
                Value="Collapsed" />
        <Setter Property="BorderBrush"
                Value="{StaticResource Timeline_SelectionRegion_Stroke}" />
        <Setter Property="Background"
                Value="{StaticResource Timeline_SelectionRegion_Fill}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:SelectionRegion">
                    <Grid>
                        <Rectangle Fill="{TemplateBinding Background}"
                                   Stroke="{TemplateBinding BorderBrush}"
                                   StrokeDashArray="3 3" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineItemContainerStyle"
           TargetType="telerik:TimelineItemContainer">
        <Setter Property="Canvas.ZIndex"
                Value="200" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerik:TimelineItemGroupsPanel Padding="0 3 0 5"
                                                     VerticalScrollBar="{Binding ElementName=Part_VerticalSlider}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadTimelineStyle1"
           TargetType="telerik:RadTimeline">
        <Setter Property="Foreground"
                Value="{StaticResource Timeline_Foreground}" />
        <Setter Property="Background"
                Value="{StaticResource Timeline_Background}" />
        <Setter Property="BorderBrush"
                Value="{StaticResource Timeline_BorderBrush}" />
        <Setter Property="BorderThickness"
                Value="1 0 1 1" />
        <Setter Property="SliderStyle"
                Value="{StaticResource Timeline_SliderStyle}" />
        <Setter Property="TimelineItemTemplate"
                Value="{StaticResource TimelineItemTemplate}" />
        <Setter Property="TimelineInstantItemTemplate"
                Value="{StaticResource TimelineInstantItemTemplate}" />
        <Setter Property="PeriodHeaderStyle"
                Value="{StaticResource TimelinePeriodHeaderStyle}" />
        <Setter Property="GroupPeriodHeaderStyle"
                Value="{StaticResource TimelineGroupPeriodHeaderStyle}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadTimeline">
                    <Border BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            SnapsToDevicePixels="True">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="130" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0"
                                    Grid.Row="0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="15" />
                                        <RowDefinition Height="0" />
                                        <RowDefinition Height="43.5" />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="1"
                                            Grid.RowSpan="2">
                                        <Border.Background>
                                            <SolidColorBrush Color="#044A6C"
                                                             Opacity="0.8" />
                                        </Border.Background>
                                        <TextBlock Background="Transparent"
                                                   VerticalAlignment="Center"
                                                   HorizontalAlignment="Center"
                                                   Foreground="#FFBF00"
                                                   FontSize="16"
                                                   FontFamily="微软雅黑"
                                                   FontWeight="Bold"
                                                   Text="{Binding RelativeSource={RelativeSource FindAncestor,AncestorType=telerik:RadTimeline},Path=Tag,StringFormat={}{0:M月d日}}" />
                                    </Border>
                                    <Grid Grid.Row="3">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="116" />
                                            <RowDefinition Height="232" />
                                            <RowDefinition Height="464" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Border Grid.Row="0">
                                            <Border Margin="0,2"
                                                    Background="{StaticResource TimeLine_Header_Background}"
                                                    BorderThickness="5,0,0,0"
                                                    BorderBrush="{StaticResource TimeLine_Header_BorderBrush}">
                                                <TextBlock Text="灾情&#x000A;舆情"
                                                           Width="80"
                                                           FontSize="26"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center"
                                                           Foreground="#FFFFFF" />
                                            </Border>
                                        </Border>
                                        <Border Grid.Row="1">
                                            <Border Margin="0,2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="88" />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                    </Grid.RowDefinitions>
                                                    <Border Grid.Row="0"
                                                            Grid.RowSpan="2"
                                                            Grid.Column="0"
                                                            Background="{StaticResource TimeLine_Header_Background}"
                                                            BorderThickness="5,0,0,0"
                                                            BorderBrush="{StaticResource TimeLine_Header_BorderBrush}">
                                                        <TextBlock Text="服务"
                                                                   VerticalAlignment="Center"
                                                                   HorizontalAlignment="Center"
                                                                   Foreground="White"
                                                                   FontSize="26" />
                                                    </Border>
                                                    <Border Grid.Row="0"
                                                            Grid.Column="1"
                                                            Background="{StaticResource TimeLine_Header_BackgroundBrush}">
                                                        <TextBlock Text="国家级"
                                                                   FontSize="24"
                                                                   TextWrapping="Wrap"
                                                                   HorizontalAlignment="Center"
                                                                   VerticalAlignment="Center"
                                                                   Foreground="#FFFFFF" />
                                                    </Border>
                                                    <Border Grid.Row="1"
                                                            Grid.Column="1"
                                                            Background="{StaticResource TimeLine_Header_BackgroundBrush}">
                                                        <TextBlock Text="省级"
                                                                   FontSize="24"
                                                                   TextWrapping="Wrap"
                                                                   HorizontalAlignment="Center"
                                                                   VerticalAlignment="Center"
                                                                   Foreground="#FFFFFF" />
                                                    </Border>
                                                </Grid>
                                            </Border>
                                        </Border>
                                        <Border Grid.Row="2">
                                            <Border Margin="0,2">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="88" />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                    </Grid.RowDefinitions>
                                                    <Border Grid.Row="0"
                                                            Grid.RowSpan="4"
                                                            Grid.Column="0"
                                                            Background="{StaticResource TimeLine_Header_Background}"
                                                            BorderThickness="5,0,0,0"
                                                            BorderBrush="{StaticResource TimeLine_Header_BorderBrush}">
                                                        <TextBlock Text="预警"
                                                                   VerticalAlignment="Center"
                                                                   HorizontalAlignment="Center"
                                                                   Foreground="White"
                                                                   FontSize="26" />
                                                    </Border>
                                                    <Border Grid.Row="0"
                                                            Grid.Column="1"
                                                            Background="{StaticResource TimeLine_Header_BackgroundBrush}">
                                                        <TextBlock Text="国家级"
                                                                   FontSize="24"
                                                                   TextWrapping="Wrap"
                                                                   HorizontalAlignment="Center"
                                                                   VerticalAlignment="Center"
                                                                   Foreground="#FFFFFF" />
                                                    </Border>
                                                    <Border Grid.Row="1"
                                                            Grid.Column="1"
                                                            Background="{StaticResource TimeLine_Header_BackgroundBrush}">
                                                        <TextBlock Text="省级"
                                                                   FontSize="24"
                                                                   TextWrapping="Wrap"
                                                                   HorizontalAlignment="Center"
                                                                   VerticalAlignment="Center"
                                                                   Foreground="#FFFFFF" />
                                                    </Border>
                                                    <Border Grid.Row="2"
                                                            Grid.Column="1"
                                                            Background="{StaticResource TimeLine_Header_BackgroundBrush}">
                                                        <TextBlock Text="市级"
                                                                   FontSize="24"
                                                                   TextWrapping="Wrap"
                                                                   HorizontalAlignment="Center"
                                                                   VerticalAlignment="Center"
                                                                   Foreground="#FFFFFF" />
                                                    </Border>
                                                    <Border Grid.Row="3"
                                                            Grid.Column="1"
                                                            Background="{StaticResource TimeLine_Header_BackgroundBrush}">
                                                        <TextBlock Text="县级"
                                                                   FontSize="24"
                                                                   TextWrapping="Wrap"
                                                                   HorizontalAlignment="Center"
                                                                   VerticalAlignment="Center"
                                                                   Foreground="#FFFFFF" />
                                                    </Border>
                                                </Grid>
                                            </Border>

                                        </Border>
                                    </Grid>
                                </Grid>
                            </Border>
                            <Border Grid.Row="0"
                                    Grid.Column="1"
                                    Height="{Binding ElementName=PART_TimelineStripLineContainer, Path=(telerik:ActualSizeProxy.ActualHeight)}"
                                    VerticalAlignment="Bottom"
                                    Background="{TemplateBinding Background}" />
                            <telerik:TimelineAnnotationsPanel x:Name="PART_TimelineAnnotationsPanel"
                                                              Grid.Row="0"
                                                              Grid.Column="1"
                                                              Grid.ColumnSpan="2"
                                                              Slider="{Binding ElementName=PART_Slider}">
                                <Grid Canvas.ZIndex="100">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="15" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <telerik:TimelineStripLineContainer x:Name="PART_TimelineStripLineContainer"
                                                                        Grid.Row="2"
                                                                        telerik:ActualSizeProxy.IsEnabled="True"
                                                                        ItemsSource="{TemplateBinding IntervalItems}">
                                        <telerik:TimelineStripLineContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <telerik:VirtualizingTimeBarPanel
                                                    Slider="{Binding ElementName=PART_Slider}" />
                                            </ItemsPanelTemplate>
                                        </telerik:TimelineStripLineContainer.ItemsPanel>
                                        <telerik:TimelineStripLineContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </telerik:TimelineStripLineContainer.Template>
                                    </telerik:TimelineStripLineContainer>
                                    <telerik:TimeSpecialSlotContainer x:Name="PART_TimeSpecialSlotContainer"
                                                                      Grid.Row="2"
                                                                      ItemsSource="{TemplateBinding SpecialSlots}">
                                        <telerik:TimeSpecialSlotContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <telerik:TimeBarPanel Slider="{Binding ElementName=PART_Slider}" />
                                            </ItemsPanelTemplate>
                                        </telerik:TimeSpecialSlotContainer.ItemsPanel>
                                        <telerik:TimeSpecialSlotContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </telerik:TimeSpecialSlotContainer.Template>
                                    </telerik:TimeSpecialSlotContainer>
                                    <Border Grid.Row="1"
                                            Style="{StaticResource TimelineLineStyle}" />
                                    <telerik:TimelineGroupPeriodContainer x:Name="PART_TimelineGroupPeriodContainer"
                                                                          Grid.Row="0"
                                                                          ItemsSource="{TemplateBinding IntervalGroups}"
                                                                          ItemHeaderStyle="{TemplateBinding GroupPeriodHeaderStyle}">
                                        <telerik:TimelineGroupPeriodContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <telerik:LabelMeasurementPanel
                                                    Slider="{Binding ElementName=PART_Slider}" />
                                            </ItemsPanelTemplate>
                                        </telerik:TimelineGroupPeriodContainer.ItemsPanel>
                                        <telerik:TimelineGroupPeriodContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </telerik:TimelineGroupPeriodContainer.Template>
                                    </telerik:TimelineGroupPeriodContainer>
                                    <telerik:TimelinePeriodContainer x:Name="PART_TimelinePeriodContainer"
                                                                     Grid.Row="1"
                                                                     Grid.RowSpan="2"
                                                                     ItemsSource="{TemplateBinding IntervalItems}"
                                                                     ItemHeaderStyle="{TemplateBinding PeriodHeaderStyle}">
                                        <telerik:TimelinePeriodContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <telerik:LabelMeasurementPanel
                                                    Slider="{Binding ElementName=PART_Slider}" />
                                            </ItemsPanelTemplate>
                                        </telerik:TimelinePeriodContainer.ItemsPanel>
                                        <telerik:TimelinePeriodContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </telerik:TimelinePeriodContainer.Template>
                                    </telerik:TimelinePeriodContainer>
                                </Grid>
                                <telerik:TimelineItemContainer x:Name="PART_TimelineItemContainer"
                                                               GroupExpandMode="{TemplateBinding GroupExpandMode}"
                                                               ItemsSource="{TemplateBinding GroupedDataItems}"
                                                               SelectionRegionControl="{Binding ElementName=PART_SelectionRegion}">
                                    <telerik:TimelineItemContainer.Template>
                                        <ControlTemplate>
                                            <Grid Background="#01FFFFFF">
                                                <ItemsPresenter />
                                            </Grid>
                                        </ControlTemplate>
                                    </telerik:TimelineItemContainer.Template>
                                </telerik:TimelineItemContainer>
                            </telerik:TimelineAnnotationsPanel>
                            <telerik:SelectionRegionPanel x:Name="PART_SelectionRegionPanel"
                                                          Grid.Row="0"
                                                          Grid.Column="1"
                                                          Slider="{Binding ElementName=PART_Slider}">
                                <telerik:SelectionRegion x:Name="PART_SelectionRegion" />
                            </telerik:SelectionRegionPanel>
                            <Border Grid.Row="1"
                                    Grid.Column="2"
                                    Visibility="{Binding Visibility, ElementName=Part_VerticalSlider}"
                                    Background="{StaticResource Timeline_Slider_Corner_Background}">
                                <Border BorderThickness="{StaticResource Timeline_Slider_Corner_BorderThickness}"
                                        Opacity="0.5"
                                        BorderBrush="{StaticResource Timeline_BorderBrush}" />
                            </Border>
                            <telerik:TimelineScrollBar x:Name="Part_VerticalSlider"
                                                       Grid.Row="0"
                                                       Grid.Column="2"
                                                       VerticalAlignment="Bottom"
                                                       Visibility="Collapsed"
                                                       ScrollBarVisibility="{TemplateBinding VerticalScrollBarVisibility}"
                                                       Style="{TemplateBinding SliderStyle}"
                                                       Height="{Binding ElementName=PART_TimelineStripLineContainer, Path=(telerik:ActualSizeProxy.ActualHeight)}"
                                                       Minimum="0"
                                                       Maximum="1"
                                                       SelectionStart="0"
                                                       SelectionEnd="1"
                                                       Orientation="Vertical"
                                                       IsDirectionReversed="True"
                                                       ThumbVisibility="Collapsed" />
                            <telerik:RadSlider x:Name="PART_Slider"
                                               Grid.Row="1"
                                               Grid.Column="0"
                                               Grid.ColumnSpan="3"
                                               Style="{TemplateBinding SliderStyle}"
                                               Width="Auto"
                                               Minimum="{Binding Path=PeriodStart, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                               Maximum="{Binding Path=PeriodEnd, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                               MinimumRangeSpan="{Binding Path=ActualMinZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                               MaximumRangeSpan="{Binding Path=MaxZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timeSpanToMaximumRangeSpanConverter}}"
                                               Selection="{Binding Path=ActualVisiblePeriodRange, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource selectionRangeConverter}}"
                                               SmallChange="{Binding Path=SmallChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                               LargeChange="{Binding Path=LargeChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                               ThumbVisibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToThumbVisibilityConverter}}"
                                               Visibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToVisibilityConverter}}"
                                               IsCoercionSuppressed="True" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerik:TimelineItemControl"
           BasedOn="{StaticResource TimelineItemControlStyle}" />
    <Style TargetType="telerik:TimelineInstantItemControl"
           BasedOn="{StaticResource TimelineInstantItemControlStyle}" />
    <Style TargetType="telerik:TimelineAnnotation"
           BasedOn="{StaticResource TimelineAnnotationStyle}" />
    <Style TargetType="telerik:SelectionRegion"
           BasedOn="{StaticResource SelectionRegionStyle}" />
    <Style TargetType="telerik:TimelineItemContainer"
           BasedOn="{StaticResource TimelineItemContainerStyle}" />

</ResourceDictionary>
                Value="{Binding Path=ItemTemplateSelector, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type telerik:RadTimeline}}}" />
        <Setter Property="DataGroups"
                Value="{Binding DataGroups}" />
        <Setter Property="ExpandMode"
                Value="{Binding GroupExpandMode, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerik:RadTimeline}}" />
        <Setter Property="Background"
                Value="Transparent" />
        <Setter Property="HorizontalContentAlignment"
                Value="Left" />
        <Setter Property="IsTabStop"
                Value="false" />
